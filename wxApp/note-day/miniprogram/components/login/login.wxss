/* components/login/login.wxss */
.user_btn {
  position: relative;
  width: 100%;
  height: 60vh;
}
.user_btn .navigator {
  position: absolute;
  z-index: 999;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
  display: inline-block;
  width: 50%;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  border-radius: 20rpx;
  border: 1px solid skyblue;
  font-size: 40rpx;
}
.user_btn .navigator .image1 {
  width: 60rpx;
  margin-right: 20rpx;
  vertical-align: middle;
}
.user_btn .image {
  position: relative;
  z-index: -1;
  width: 100%;
  height: 100%;
  filter: blur(10rpx);
}